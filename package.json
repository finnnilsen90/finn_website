{
  "name": "finn_infrastructure",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/testing/setupjest.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testing/filemock.js",
      "\\.(css|less)$": "<rootDir>/testing/styleMock.js"
    },
    "moduleFileExtensions": [
      "json",
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "automock": false,
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "version": "1.0.0",
  "description": "This is Finn's general node infrastructure for createing modules.",
